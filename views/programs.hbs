<div class="row">
    <div class="col-md-12">
        <h2>Programs</h2>
        <hr />

        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Program Code</th>
                        <th>Program Name</th>
                    </tr>
                </thead>
            </table>
        </div>

        <tbody>
            {{#each program}}
            <tr>
                <td><a href="/students?program={{programCodes}}">{{programCodes}}</a></td>
                <td><a href="/students?program={{programCodes}}">{{programName}}</a></td>            
            </tr>
            {{/each}}
        </tbody>
    </div>
</div>